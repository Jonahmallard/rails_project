    <%= form_for race do |f| %>
      <%= f.label "Race Type:" %>
        <%= f.text_field :race_type %>
      <%= f.label "Location:" %>
        <%= f.text_field :location %>
      <%= f.label "Date (MM/DD/YYYY):" %>
        <%= f.text_field :date %>
      <%= f.label "Number Placed 1-300:" %>
        <%= f.number_field :placement %>
      <%= f.submit %>
    <% end %>